func main(argc i32, argv u8) i32 {
	a := 0;
	b := 24;
	printf("Please enter a number:\n");
	while a != b {
		scanf("%d", #a);	
		if a > b {
			printf("Too high. Enter another:\n");
		}
		if a < b {
			printf("Too low. Enter another:\n");
		}
	}
	printf("Correct!");
	return 0;
}
